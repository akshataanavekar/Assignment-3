        <errorMessage>Age should be between 21 years to 31 years</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_cannot_be_older_than_18_years</fullName>
        <active>true</active>
        <errorConditionFormula>IF(YEAR(TODAY()) - YEAR( Date_of_Birth__c ) &lt; 18, true, false)</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>“Please provide a valid Date of birth. Employee cannot be less than 18 years of Age</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_SSN_size</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(TEXT(SSN__c)) &lt;&gt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>Length of SSN number should be 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_date</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Notice_Period__c, IF(ISBLANK(Resignation_Date__c), true, false),IF(ISBLANK(Resignation_Date__c), false, true) )</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Risignation date is required</errorMessage>
    </validationRules>
</CustomObject>
